{
  "name": "nodb",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@google-cloud/datastore": "^4.4.0",
    "axios": "^0.18.1",
    "datastore": "^1.8.2",
    "express": "^4.17.1",
    "nodemon": "1.19.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build-start": "npm run build; PORT=5050 node index.js",
    "debug": "npm run build; PORT=5050 node inspect index.js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "server/index.js",
  "nodemonConfig": {
    "ignore": [
      "src/*",
      "public/*"
    ]
  },
  "proxy": "http://localhost:5050"
}
